{
  "name": "plasma-js-lib",
  "version": "0.0.2",
  "description": "PG Plasma JS client library",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha --recursive test/",
    "test.coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "build": "gulp",
    "lint": "./node_modules/.bin/eslint src/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && npm test && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,json,css,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:plasma-group/plasma-js-lib.git"
  },
  "keywords": [
    "plasma"
  ],
  "author": "Plasma Group",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plasma-group/plasma-js-lib/issues"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "coveralls": "^3.0.2",
    "del": "^3.0.0",
    "eslint": "^5.10.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "exorcist": "^1.0.1",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify-es": "^1.0.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "nyc": "^13.1.0",
    "vinyl-source-stream": "^2.0.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "uuid": "^3.3.2"
  }
}
